{
  "name": "@shuvi/error-overlay",
  "version": "1.0.0-rc.4",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "dev": "run-p watch:*",
    "watch:cjs": "tsc -p tsconfig.build.json -m commonjs --outDir lib -w",
    "watch:esm": "tsc -p tsconfig.build.json -m esnext --outDir esm -w",
    "watch:umd": "tsc -p tsconfig.build.json -m umd --outDir umd -w",
    "prebuild": "rimraf lib",
    "build": "run-p build:*",
    "build:cjs": "tsc -p tsconfig.build.json -m commonjs --outDir lib",
    "build:ems": "tsc -p tsconfig.build.json -m esnext --outDir esm",
    "build:umd": "rollup -c rollup.config.js"
  },
  "engines": {
    "node": ">= 12.0.0"
  },
  "dependencies": {
    "anser": "1.4.9",
    "html-entities": "2.3.2",
    "stacktrace-parser": "0.1.10",
    "tslib": "^2.4.0",
    "strip-ansi": "6.0.1",
    "launch-editor": "2.2.1"
  },
  "devDependencies": {
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.6",
    "@types/rollup": "^0.54.0",
    "rollup": "^2.77.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript2": "^0.32.1"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  }
}
